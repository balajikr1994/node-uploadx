{
  "name": "node-uploadx",
  "version": "1.0.0",
  "description": "Node.js middleware for handling resumable uploads",
  "keywords": [
    "upload",
    "chunk",
    "express",
    "middleware"
  ],
  "author": {
    "name": "kukhariev",
    "email": "q_h@inbox.ru"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kukhariev/node-uploadx.git"
  },
  "license": "MIT",
  "engines": {
    "npm": ">= 6.0.0"
  },
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "start": "DEBUG=uploadx* ts-node test/server.ts",
    "dev": "./node_modules/nodemon/bin/nodemon.js",
    "clean": "rimraf lib",
    "build": "npm run tslint && npm run clean && npm run build-ts",
    "test": "./node_modules/.bin/mocha",
    "test:watch": "./node_modules/.bin/mocha --watch",
    "build-ts": "tsc",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "demo": "DEBUG=uploadx* node example/index.js",
    "preversion": "npm run build",
    "postversion": "git push --follow-tags && npm publish"
  },
  "devDependencies": {
    "@types/bytes": "^3.0.0",
    "@types/chai": "^4.1.3",
    "@types/chai-http": "^3.0.4",
    "@types/cors": "^2.8.4",
    "@types/debug": "0.0.30",
    "@types/express": "^4.16.0",
    "@types/http-errors": "^1.6.1",
    "@types/mocha": "^5.2.1",
    "@types/node": "^10.3.1",
    "body-parser": "^1.18.3",
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "mocha": "^5.2.0",
    "nodemon": "^1.17.5",
    "rimraf": "^2.6.2",
    "ts-node": "^6.1.0",
    "tslint": "^5.10.0",
    "typescript": "^2.9.1"
  },
  "dependencies": {
    "bytes": "^3.0.0",
    "debug": "^3.1.0",
    "http-errors": "^1.6.3",
    "raw-body": "^2.3.3"
  }
}
